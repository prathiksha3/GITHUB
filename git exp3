 git init
 git clone https://github.com/prathiksha3/gitexp3.git
 cd gitexp3
git fetch origin
git rebase origin/main
echo "hello" > t1.txt
git branch feature-branch
 git checkout feature-branch
 echo "hello world" > t2.txt
 git add .
 git add t2.txt
 git commit -m "changed t1.txt"
git checkout main
git merge feature-branch
git push
